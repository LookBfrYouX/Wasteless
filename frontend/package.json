{
  "name": "seng302-team-300",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 9500",
    "build": "vue-cli-service build --mode staging",
    "build-prod": "vue-cli-service build --mode prod",
    "kill-all": "fuser -k 9500/tcp 9499/tcp",
    "lint": "vue-cli-service lint",
    "test": "jest --coverage",
    "sonarqube": "node sonarqube-analysis.js"
  },
  "dependencies": {
    "@vue/test-utils": "^1.1.3",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.4",
    "core-js": "^3.8.3",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "serve": "^11.3.2",
    "vue": "^2.6.12",
    "vue-barcode-reader": "^0.0.3",
    "vue-chartjs": "^3.5.1",
    "vue-router": "^3.5.1",
    "vuejs-logger": "^1.5.5",
    "vuetify": "^2.5.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-service": "~4.5.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.19.0",
    "eslint-plugin-vue": "^7.5.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "sonarqube-scanner": "^2.8.0",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js",
      "jest-canvas-mock"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.vue$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!vue-barcode-reader)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/fileMock.js",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^#/(.*)$": "<rootDir>/test/$1"
    }
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
